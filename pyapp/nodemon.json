{
  "restartable": "rs",
  "ignore": [
    "pyenv/",
    "../.git",
    "../.gitgnore",
    "../.vscode",
    "../gui/",
    "../node_modules/",
    "../README.md"
  ],
  "verbose": true,
  "execMap": {
    "py": "run-script-os"
  },
  "watch": [
    "pyapp/",
    "api/",
    "main.py"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "py",
  "events": {
    "start": "clear || cls",
    "restart": "clear || cls",
    "exit": "clear || cls"
  }
}